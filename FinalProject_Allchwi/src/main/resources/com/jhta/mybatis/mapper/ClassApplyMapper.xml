<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.mapper.ClassApplyMapper">
	<insert id="insert" parameterType="classapply">
		insert into class_apply values(#{apply_num},#{date_num},#{ml_num},
						#{stu_lev},#{stu_msg},#{sut_phone},#{apply_auth},now(),#{class_finish})
	</insert>
	<select id="list" parameterType="int" resultType="classApply">
		select date_num, class_num, class_date, class_startTime, class_endDate, class_comment, class_msg, class_regdate
		from class_date
	</select>
</mapper>

